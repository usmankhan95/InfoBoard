@model OnlineInfoService.Models.Report
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
<div>
    <h4>Published: @Model.Date.ToShortDateString()</h4>
    <hr/>
    <dl class="dl-horizontal">
        <dt>Author:</dt>
        <dd><a href="@Url.Action("Details", "Member", new {id = @Model.Member.Id})">@Model.Member.Name</a></dd>
        <dt>Subject:</dt>
        <dd>@Model.Subject.Name</dd>
@*        <dt>File:</dt>*@
@*        <dd><img src="@Url.Content(Model.)" alt="Image" height="50" width="50" /></dd>*@
    </dl>
</div>

